<h2>Cats for sale:</h2>

<ul>
    <% @cats.each do |cat| %>
    <h3><a href="cat_url(cat)"> Name: <%= cat.name %> </a></h3>
        <li>Color: <%= cat.color %></li>
        <li>
            Sex: <%= cat.sex %>
        </li>
        <li>
            Age: <%= cat.age %>
        </li>
        <li>
            Description: <%= cat.description  %>
        </li>
    <% end %>
</ul>

